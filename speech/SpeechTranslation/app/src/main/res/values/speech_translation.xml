<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- The Google Cloud Function that provides speech translation capabilities.
         This value should be in the form:
         https://[GCF_REGION]-[GOOGLE_CLOUD_PROJECT].cloudfunctions.net/[FUNCTION_NAME]
         For example:
         https://us-central1-myproject-18143.cloudfunctions.net/speechTranslate
         See https://github.com/GoogleCloudPlatform/nodejs-docs-samples/tree/master/functions/speech-to-speech
         for more information. Setting this value enables speech translation capabilities
         on the sample. -->
    <string
        name="speechToSpeechEndpoint"
        formatted="false"
        translatable="false">https://us-central1-YOUR-PROJECT-ID.cloudfunctions.net/speechTranslate</string>
    <!-- OAuth 2.0 scope to allow access to read data to Google Cloud Storage buckets.
         This allows the sample to download files with translated audio messages. -->
    <string
        name="speechToSpeechOAuth2Scope"
        formatted="false"
        translatable="false">https://www.googleapis.com/auth/devstorage.read_only</string>
    <!-- The Google Cloud Storage base endpoint to download files from a bucket. -->
    <string
        name="gcsBaseEndpoint"
        formatted="false"
        translatable="false">https://storage.googleapis.com/</string>
    <string
        name="gcpClientID"
        formatted="false"
        translatable="false">YOUR-CLIENT-ID</string>
</resources>